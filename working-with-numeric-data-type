print("Python has three numeric types: int, float, and complex")



# Ini catatan saya
myString = "Santika"
print(type(myString))

print("=== Integer (Bilangan Bulat) ===")
myInt = 19
print(type(myInt))

print("=== Float (Bilangan Float) ===")
myFloat = 19.5
print(type(myFloat))

print("=== Boolean ===")
myBool = True
print(type(myBool))